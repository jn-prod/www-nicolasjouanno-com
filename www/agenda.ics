---
layout: null
permalink: /agenda.ics
---
BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//Nicolas Jouanno//nicolasjouanno.com//FR
CALSCALE:GREGORIAN
METHOD:PUBLISH
{% for event in site.events %}
BEGIN:VEVENT
UID:{{ event.slug }}@nicolasjouanno.com
DTSTAMP:{{ site.time | date: "%Y%m%dT%H%M%SZ" }}
DTSTART:{{ event.start | date: "%Y%m%dT%H%M%S" }}
{% if event.end %}DTEND:{{ event.end | date: "%Y%m%dT%H%M%S" }}{% endif %}
SUMMARY:{{ event.title | strip_newlines | escape }}
DESCRIPTION:{{ event.description | strip_newlines | escape }}
LOCATION:{{ event.location | strip_newlines | escape }}
URL:{{ event.url | absolute_url }}
END:VEVENT
{% endfor %}
END:VCALENDAR

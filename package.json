{
  "name": "www-nicolasjouanno-com",
  "version": "1.0.0",
  "description": "Portfolio de Nicolas Jouanno",
  "main": "client/src/index.ts",
  "scripts": {
    "postinstall": "cd client && npm ci",
    "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
    "dev:server": "bundle exec jekyll serve --livereload",
    "dev:client": "cd client && npm run dev",
    "client:build": "rm -rf www/assets && cd client && npm run build && rm dist/index.html && cp -R dist ../www/assets"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jn-prod/www-nicolasjouanno-com"
  },
  "author": "nicolas jouanno",
  "license": "ISC",
  "homepage": "https://nicolasjouanno.com",
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}

{
  "name": "www-nicolasjouanno-com",
  "version": "1.0.0",
  "description": "Portfolio de Nicolas Jouanno",
  "main": "client-v2/src/index.ts",
  "scripts": {
    "dev": "concurrently \"npm run server:dev\" \"npm run client:dev\"",
    "dev:server": "bundle exec jekyll serve --livereload",

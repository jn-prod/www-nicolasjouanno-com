{
  "name": "www-nicolasjouanno-com",
  "version": "1.0.0",
  "description": "Portfolio de Nicolas Jouanno",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jn-prod/www-nicolasjouanno-com"
  },
  "author": "nicolas jouanno",
  "license": "ISC",
  "homepage": "https://nicolasjouanno.com",
  "scripts": {
    "dev": "turbo dev",
    "build": "turbo build",
    "build:prod": "run-s build bundle-packages",
    "build:preview": "python3 -m http.server -d ./dist",
    "change": "changeset",
    "release": "changeset version",
    "lint": "turbo lint",
    "lint:fix": "turbo lint:fix",
    "clean-dist": "rm ./dist/package.json && rm ./dist/CHANGELOG.md",
    "bundle-packages": "rm -rf ./dist && cp -R ./site/www/_site/ ./dist && cp -R ./packages/client/dist/assets/ ./dist/assets && pnpm clean-dist"
  },
  "devDependencies": {
    "@changesets/cli": "^2.26.1",
    "npm-run-all": "^4.1.5",
    "turbo": "^1.8.5"
  }
}